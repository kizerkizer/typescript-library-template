    "build:ts:node": "tsc --project ./tsconfig/tsconfig.commonjs.json",
    "build:ts:esm": "tsc --project ./tsconfig/tsconfig.esm.json",
    "build:tests": "tsc --project ./tsconfig/tsconfig.tests.json",
    "bundle": "rollup --config",
    "clean": "rm -rf ./dist && mkdir ./dist"
