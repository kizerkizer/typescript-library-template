  "displayName": "%name%",
  "description": "",
  "devDependencies": {
    "@types/node": "^10.9.4",
    "camelcase": "^5.0.0",
    "rollup": "^0.65.2",
    "rollup-plugin-node-resolve": "^3.4.0",
    "tslib": "^1.9.3",
